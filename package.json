{
    "name": "@teamhive/npm-cleanup",
    "version": "0.0.1",
    "private": false,
    "description": "A cli tool to automate cleanup and updating of node projects",
    "keywords": [
        "npm",
        "node",
        "project",
        "cleanup"
    ],
    "license": "ISC",
    "author": "TeamHive",
    "contributors": [
        "Josh Gibson <gibsonjoshua55>"
    ],
    "homepage": "https://github.com/TeamHive/npm-cleanup",
    "repository": {
        "type": "git",
        "url": "https://github.com/TeamHive/npm-cleanup.git"
    },
    "bugs": {
        "url": "https://github.com/TeamHive/npm-cleanup/issues"
    },
    "bin": {
        "npm-cleanup": "./bin/npm-cleanup.js"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "watch": "tsc -w",
        "prestart": "rm -rf ./lib && tslint -p tsconfig.json -c tslint.json",
        "start": "tsc",
        "prepack": "tsc",
        "postpublish": "git push && git push --tags",
        "postpack": "rm -rf ./lib",
        "generateSchemaType": "npm run start && node lib/scripts/generate-ts-types.js"
    },
    "dependencies": {
        "ajv": "^6.10.0",
        "colors": "^1.3.3",
        "commander": "^2.20.0",
        "deepmerge": "^3.3.0"
    },
    "devDependencies": {
        "@types/node": "^12.0.10",
        "json-schema-to-typescript": "^6.1.3",
        "tslint": "^5.18.0",
        "typescript": "^3.1.6"
    }
}
